<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:m="sap.m" xmlns:l="sap.ui.layout" xmlns:u="sap.ui.unified"
		controllerName="sap.ui.demo.toolpageapp.controller.settings.EmployeeImport" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Employee Import">
		<content>
	<m:Panel header="Promotion And Alumni">
				<l:VerticalLayout>
				
		    <u:FileUploader
			id="fileUploader1"
			name="myFileUpload"
			uploadUrl="upload/"
			width="400px"
			tooltip="Upload your file to the local server"
			uploadComplete="handleUploadComplete"/>
		    <Button
			text="Submit"
			press="handleUploadPress"/>
	           </l:VerticalLayout>
				
				
				
				 </m:Panel>
		</content>
	</Page>
</core:View>