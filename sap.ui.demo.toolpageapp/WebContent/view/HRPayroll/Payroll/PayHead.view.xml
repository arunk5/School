<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
		controllerName="sap.ui.demo.toolpageapp.controller.HRPayroll.Payroll.PayHead" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="Payroll">
		<content>
	<GenericTile id="Tile1"
				class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Pay Head" 
				press="pressCourse">
				<tileContent>
					<TileContent>
						<content>
							<ImageContent src="sap-icon://course-program"></ImageContent>
						</content>
					</TileContent>
				</tileContent>
			</GenericTile>

			<GenericTile id="Tile2"
				class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Payment Types" failedText = "HRPayroll/Payroll/PaymentsType"
				press="pressCourse">
				<tileContent>
					<TileContent>
						<content>
							<ImageContent src="sap-icon://course-program"></ImageContent>
						</content>
					</TileContent>
				</tileContent>
			</GenericTile>

			<GenericTile id="Tile3"
				class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Salary Settings" failedText = "HRPayroll/Payroll/SalarySettings"
				press="pressCourse">
				<tileContent>
					<TileContent>
						<content>
							<ImageContent src="sap-icon://course-program"></ImageContent>
						</content>
					</TileContent>
				</tileContent>
			</GenericTile>
		
		<GenericTile id="Tile4"
				class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header=" Employee Salary" failedText = "HRPayroll/Payroll/EmployeeSalary"
				press="pressCourse">
				<tileContent>
					<TileContent>
						<content>
							<ImageContent src="sap-icon://course-program"></ImageContent>
						</content>
					</TileContent>
				</tileContent>
			</GenericTile>
			<GenericTile id="Tile5"
				class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Generate Payslip" failedText = "HRPayroll/Payroll/GeneratePayslip"
				press="pressCourse">
				<tileContent>
					<TileContent>
						<content>
							<ImageContent src="sap-icon://course-program"></ImageContent>
						</content>
					</TileContent>
				</tileContent>
			</GenericTile>
		</content>
	</Page>
</core:View>