<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
		controllerName="sap.ui.demo.toolpageapp.controller.Academic.CourseBatch.Course" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title=" Course Selection ">
		<content>
	<GenericTile id="Tile1" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Course"
		failedText = "Academic/CourseBatch/CourseView" press="pressCourse">
		<tileContent>
			<TileContent>
				<content>
					<ImageContent src="sap-icon://course-program"></ImageContent>
				</content>
			</TileContent>
		</tileContent>
	</GenericTile>
	
	<GenericTile id="Tile2" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Batch"
		failedText = "Academic/CourseBatch/Batch" press="pressBatch">
		<tileContent>
			<TileContent>
				<content>
					<ImageContent src="sap-icon://group"></ImageContent>
				</content>
			</TileContent>
		</tileContent>
	</GenericTile>
	
	<GenericTile id="Tile3" class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Teacher Allocation"
		failedText = "Academic/CourseBatch/ClassTeacherAllocation" press="pressTeacher">
		<tileContent>
			<TileContent>
				<content>
					<ImageContent src="sap-icon://learning-assistant"></ImageContent>
				</content>
			</TileContent>
		</tileContent>
	</GenericTile>
	
	
		</content>
	</Page>
</core:View>