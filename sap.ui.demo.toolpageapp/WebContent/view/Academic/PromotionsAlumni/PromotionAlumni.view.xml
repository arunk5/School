				<tileContent>
					<TileContent>
						<content>
							<ImageContent src="sap-icon://course-program"></ImageContent>
						</content>
					</TileContent>
				</tileContent>
			</GenericTile>

			<GenericTile id="Tile2"
				class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Alumni" failedText="Academic/PromotionsAlumni/AlumniMemb"
				press="Alumni">
				<tileContent>
					<TileContent>
						<content>
							<ImageContent src="sap-icon://course-program"></ImageContent>
						</content>
					</TileContent>
				</tileContent>
			</GenericTile>
	
	
	
	
	
		</content>
	</Page>
</core:View>