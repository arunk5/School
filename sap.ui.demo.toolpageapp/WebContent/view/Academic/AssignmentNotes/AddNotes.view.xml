<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
		controllerName="sap.ui.demo.toolpageapp.controller.Academic.AssignmentNotes.AddNotes" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title=" Assignments Section ">
		<content>
	
				<GenericTile id="Tile1"
				class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Add Assignment" failedText = "Academic/AssignmentNotes/AddAssignmentView"
				press="AddAssignment">
				<tileContent>
					<TileContent>
						<content>
							<ImageContent src="sap-icon://course-program"></ImageContent>
						</content>
					</TileContent>
				</tileContent>
			</GenericTile>

			<GenericTile id="Tile2"
				class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="Add Notes" failedText = "Academic/AssignmentNotes/AddAssignment"
				press="AddNotes">
				<tileContent>
					<TileContent>
						<content>
							<ImageContent src="sap-icon://course-program"></ImageContent>
						</content>
					</TileContent>
				</tileContent>
			</GenericTile>

			<GenericTile id="Tile3"
				class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" header="AssignmentImport" failedText = "Academic/AssignmentNotes/AssignmentImport"
				press="AssignmentImport">
				<tileContent>
					<TileContent>
						<content>
							<ImageContent src="sap-icon://course-program"></ImageContent>
						</content>
					</TileContent>
				</tileContent>
			</GenericTile>
	
	
	
	
	
	
	
		</content>
	</Page>
</core:View>